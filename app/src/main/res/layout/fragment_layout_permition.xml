<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent">



        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                        <cn.demomaster.huan.quickdeveloplibrary.widget.button.QDButton
                            android:id="@+id/btn_write_external_storage"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="存储权限"/>
                        <cn.demomaster.huan.quickdeveloplibrary.widget.button.QDButton
                            android:id="@+id/btn_write_external_storage2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="存储权限2"/>
                        <cn.demomaster.huan.quickdeveloplibrary.widget.button.QDButton
                            android:id="@+id/btn_error_buggly"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/select_font"/>

                </LinearLayout>
        </ScrollView>

</RelativeLayout>
