<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="6dp"
    android:paddingRight="6dp"
    android:background="@color/green_light">

    <cn.demomaster.huan.quickdeveloplibrary.widget.ImageTextView
        android:id="@+id/it_actionbar_common_left"
        android:layout_width="@dimen/activity_actionbar_button_width"
        android:layout_height="match_parent"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="@dimen/quickdev_title_text_size"
        android:gravity="center_vertical"
        android:src="@drawable/ic_warning_black_24dp" />

    <TextView
        android:id="@+id/it_actionbar_common_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="55dp"
        android:layout_marginRight="55dp"
        android:gravity="left|center_vertical"
        android:layout_gravity="center"
        android:singleLine="true"
        android:text="123"
        android:textColor="@color/black"
        android:textSize="@dimen/quickdev_title_text_size" />


</FrameLayout>
